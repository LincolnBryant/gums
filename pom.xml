<?xml version="1.0" encoding="UTF-8"?>
<project>
	<modelVersion>4.0.0</modelVersion>
	<groupId>gums</groupId>
	<artifactId>gums-service</artifactId>
	<packaging>war</packaging>
	<version>1.2.0</version>
	<description></description>
  
  	<repositories>
		<repository>
	    	<id>central</id>
			<name>Maven Repository Switchboard</name>
	 		<layout>default</layout>
 			<url>http://repo1.maven.org/maven2</url>
		</repository>
		<repository>
	    	<id>usatlas</id>
			<name>usatlas repository</name>
	 		<layout>default</layout>
 			<url>http://grid.racf.bnl.gov/mvn/</url>
		</repository>
	</repositories>
 
	<distributionManagement>
		<repository>
	      <id>usatlas</id>
	      <name>usatlas repository</name>
	      <url>scp://grid.racf.bnl.gov:/var/www/html/mvn</url>
		</repository>
	</distributionManagement>	

	<!--<scm>
	    <developerConnection>scm:svn:https://svn.usatlas.bnl.gov/svn/privilege/gums-service</developerConnection>
	</scm>-->

  	<build>
		<plugins>
	 		<plugin>
				<artifactId>maven-antrun-plugin</artifactId>
				<executions>
					<execution>
						<id>1</id>
						<phase>compile</phase>
						<configuration>
							<tasks>
								<mkdir dir="target/scripts"/>
				                <copy todir="target/scripts">
				                	<fileset dir="src/main/scripts/"/>
				                </copy>
				                <mkdir dir="target/config"/>
				                <copy todir="target/config">
				                	<fileset dir="src/main/config/"/>
				                </copy>
				                <mkdir dir="target/sql"/>
				                <copy todir="target/sql">
				                	<fileset dir="src/main/sql/"/>
				                </copy>
				                <mkdir dir="target/xslt"/>
				                <copy todir="target/xslt">
				                	<fileset dir="src/main/xslt/"/>
				                </copy>
				                <copy todir="target/classes/">
				                	<fileset includes="gums.config*" dir="src/main/config/"/>
				                </copy>
							</tasks>
						</configuration>
						<goals>
							<goal>run</goal>
						</goals>
					</execution>
				</executions>
			</plugin> 	
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<excludes>
						<exclude>**/.svn</exclude>
					</excludes>
				</configuration>
			</plugin>
			<plugin>
		        <groupId>org.apache.maven.plugins</groupId>
		        <artifactId>maven-surefire-plugin</artifactId>
		        <configuration>
			      <excludes>
		            <exclude>**/.svn</exclude>
		          </excludes>
		        </configuration>
			</plugin>
	    </plugins>
	</build>
  
