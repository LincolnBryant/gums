<?xml version="1.0" encoding="utf-8"?>
<project basedir="./" name="GUMS">
    <target name="createClientStub">
        <path id="axis.classpath">
            <fileset dir="${basedir}/target/temp">
                <include name="*.jar"/>
            </fileset>
        </path>
        <taskdef classpathref="axis.classpath" resource="axis-tasks.properties"/>
        <axis-java2wsdl classname="gov.bnl.gums.admin.GUMSAPI" implClass="gov.bnl.gums.admin.GUMSAPIImpl" location="http://localhost:8080/gums/admin" namespace="http://www.bnl.gov/namespaces/2004/09/gums/GUMSService" output="${basedir}/target/temp/gums.wsdl">
            <mapping namespace="http://www.bnl.gov/namespaces/2004/09/gums/GUMSService" package="gov.bnl.gums.admin"/>
            <classpath refid="maven.dependency.classpath"/>
        </axis-java2wsdl>
        <axis-wsdl2java deployscope="Application" output="${basedir}/src/main/java" serverside="false" url="${basedir}/target/temp/gums.wsdl">
            <mapping namespace="http://www.bnl.gov/namespaces/2004/09/gums/GUMSService" package="gov.bnl.gums.admin"/>
        </axis-wsdl2java>
    </target>
</project>
