<gums allowGridmapFiles="false">
    <persistenceFactories>

        <!-- 
        MySQL setup - The parts of the configuration that uses mysql only will use this 
        persistenceFactory. Typically everything except account pools will be MySQL only. 
        -->
        
        <persistenceFactory 
	        name="mysql" 
	        className="gov.bnl.gums.hibernate.HibernatePersistenceFactory" 
	        hibernate.connection.driver_class="com.mysql.jdbc.Driver" 
	        hibernate.dialect="net.sf.hibernate.dialect.MySQLDialect" 
            hibernate.c3p0.min_size="3" 
	        hibernate.c3p0.max_size="20" 
	        hibernate.c3p0.timeout="180" 
            hibernate.connection.url="jdbc:mysql://localhost:3306/GUMS_3" 
	        hibernate.connection.username="gums" 
	        hibernate.connection.password="" 
	        hibernate.connection.autoReconnect="true"/>

        <!-- 
        LDAP/MySQL setup - For managing the pool, we are using a hybrid LDAP/BNL 
        persistence mechanism. Most of the information (such as which accounts are mapped 
        to what DN and which accounts are free) are kept on MySQL. LDAP is only used to 
        modify the primary and secondary groups 
        -->

            <persistenceFactory 
                name="bnl" 
                className="gov.bnl.gums.BNLPersistenceFactory" 
                mysql.jdbcDriver="com.mysql.jdbc.Driver"
                mysql.hibernate.connection.driver_class="com.mysql.jdbc.Driver"
                mysql.hibernate.dialect="net.sf.hibernate.dialect.MySQLDialect"
                mysql.hibernate.c3p0.min_size="3"
                mysql.hibernate.c3p0.max_size="20"
                mysql.hibernate.c3p0.timeout="180"
                mysql.hibernate.connection.url="jdbc:mysql://localhost:3306/GUMS_3"
                mysql.hibernate.connection.username="gums"
                mysql.hibernate.connection.password=""
                mysql.hibernate.connection.autoReconnect="true"
                ldap.java.naming.provider.url="ldap://rldap01.rcf.bnl.gov/dc=racf,dc=bnl,dc=gov"
                ldap.java.naming.factory.initial="com.sun.jndi.ldap.LdapCtxFactory"
                ldap.java.naming.security.authentication="simple"
                ldap.java.naming.security.principal="uid=gumsAdmin,ou=People,dc=racf,dc=bnl,dc=gov"
                ldap.java.naming.security.credentials=""
                synchGroups="false"
            />
        </persistenceFactories>
        
        
